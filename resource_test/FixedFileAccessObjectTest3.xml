<?xml version="1.0" encoding="UTF-8"?>
<define type="FixedFile" name="サンプルファイル定義" >
    <common dir="D:/tmp"
        file="Sample.txt"
        encode="UTF-8"
        lineFeed="LF" />
    <header>
        <column id="date" name="基準日" required="true" length="10" padding="RPad" />
        <column id="count" name="レコード件数" required="true" length="10" padding="LPad" />
    </header>
    <body>
        <column id="userId" name="ユーザーID" required="true" length="20" padding="RPad" />
        <column id="userName" name="ユーザー名" required="true" length="50" padding="RPad" />
        <column id="password" name="パスワード" required="true" length="128" padding="RPad" />
        <column id="tel" name="電話番号" required="false" length="50" padding="LPad" />
        <column id="mailAdress" name="メールアドレス" required="false" length="255" padding="NRPad" />
        <column id="remarks" name="備考" required="false" length="255" padding="NLPad" />
    </body>
    <futter>
    </futter>
</define>