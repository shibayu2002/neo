<?xml version="1.0" encoding="UTF-8"?>
<define name="サンプルファイル定義" >
    <common dir="D:/tmp"
        file="Sample.txt"
        encode="MS932"
        lineFeed="CRLF" />
    <header>
        <column id="date" name="基準日" required="True" length="8" padding="RPad" />
        <column id="count" name="レコード件数" required="True" length="10" padding="LPad" />
    </header>
    <body>
        <column id="userId" name="ユーザーID" required="True" length="20" padding="RPad" />
        <column id="userName" name="ユーザー名" required="True" length="50" padding="RPad" />
        <column id="password" name="パスワード" required="True" length="128" padding="RPad" />
        <column id="tel" name="電話番号" required="False" length="50" padding="LPad" />
        <column id="mailAdress" name="メールアドレス" required="False" length="255" padding="RPad" />
        <column id="remarks" name="備考" required="False" length="255" padding="LPad" />
    </body>
    <futter>
    </futter>
</define>